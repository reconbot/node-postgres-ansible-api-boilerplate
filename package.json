    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "express-routebuilder": "^2.1.0",
    "is-my-json-valid": "^2.13.1",
    "morgan": "^1.7.0",
    "pg-monitor": "^0.5.11",
    "pg-promise": "^5.3.5",
    "supervisor": "^0.11.0"
  }
